{
    "name": "gamegos/nosql-couchbase",
    "description": "Couchbase extension for gamegos/nosql library",
    "keywords": [
        "nosql",
        "key value",
        "cache",
        "couchbase"
    ],
    "license": "MIT",
    "require": {
        "php": "~5.5 || 7.0.*",
        "gamegos/nosql": "^0.2",
        "ext-couchbase": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Gamegos\\NoSql\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8 - 5.5",
        "gamegos/php-code-sniffer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Gamegos\\NoSql\\Tests\\": [
                "tests/",
                "vendor/gamegos/nosql/tests/"
            ]
        }
    },
    "authors": [
        {
            "name": "Safak Ozpinar",
            "email": "safak@gamegos.com"
        }
    ]
}
